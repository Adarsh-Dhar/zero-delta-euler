generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Pool {
  id           String   @id @default(uuid())
  token0       String
  token1       String
  feeTier      Float
  tvl          Float    @default(0)
  volume24h    Float    @default(0)
  apr          Float    @default(0)
  concentration String
  owner        String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
